{
    "name": "codebuglab/laravel-opensubtitles",
    "description": "Simple integration with OpenSubtitles API to search for and download subtitles.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Elkomy",
            "email": "mo.ah.elkomy@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "illuminate/support": ">=6.0.0",
        "guzzlehttp/guzzle": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "CodeBugLab\\OpenSubtitles\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodeBugLab\\OpenSubtitles\\OpenSubtitlesServiceProvider"
            ]
        }
    },
    "keywords": [
        "opensubtitles",
        "opensubtitles-api",
        "download-subtitles",
        "subtitles-downloader",
        "subtitles",
        "subtitle",
        "subtitles-search",
        "api",
        "php",
        "laravel"
    ]
}
